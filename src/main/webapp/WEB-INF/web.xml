<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>com.company.web.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>instructor</servlet-name>
    <servlet-class>com.company.web.InstructorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>instructor</servlet-name>
    <url-pattern>/no-teach/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>course</servlet-name>
    <servlet-class>com.company.web.CourseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>course</servlet-name>
    <url-pattern>/course-no-teacher/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>job</servlet-name>
    <servlet-class>com.company.web.JobServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>job</servlet-name>
    <url-pattern>/job-no-employee/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>employee</servlet-name>
    <servlet-class>com.company.web.EmployeeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>employee</servlet-name>
    <url-pattern>/employees/*</url-pattern>
  </servlet-mapping>
</web-app>
